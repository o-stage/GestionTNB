<Window x:Class="TaxesV1.MainWindow"
          xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:TaxesV1"
        xmlns:r="clr-namespace:TaxesV1.Resources"
        xmlns:converter="clr-namespace:TaxesV1.MultiValueConverters"
        xmlns:fa="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp"
        mc:Ignorable="d"
        Title="Taxes Sure LesTerrains Non Batu" Width="1000" Height="580" VerticalContentAlignment="Top" WindowStartupLocation="CenterScreen">

    <Window.Resources>
        <ResourceDictionary>
            <converter:DifferenceConverter x:Key="DifferenceConverter" />
            <converter:AccountableTypeConverter x:Key="AccountableTypeConverter" />
        </ResourceDictionary>
    </Window.Resources>
    <DockPanel x:Name="Body" Background="#ffffff">
        <local:MainMenu DockPanel.Dock="Left" HorizontalAlignment="Left" />
    </DockPanel>
</Window>
